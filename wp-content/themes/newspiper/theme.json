{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "blue",
					"color": "#1656d1",
					"name": "Blue"
				},
				{
					"slug": "yellow",
					"color": "#ffd100",
					"name": "Yellow"
				},
				{
					"slug": "green",
					"color": "#5abeb2",
					"name": "Green"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000",
					"name": "Black"
				},
				{
					"slug": "text-primary",
					"color": "#404040",
					"name": "Text Primary"
				},
				{
					"slug": "links",
					"color": "#387cff",
					"name": "Links"
				}
			]
		},
		"layout": {
			"contentSize": "920px",
			"wideSize": "1180px"
		},
		"custom": {
			"spacing": {
				"small": "clamp(.5rem, 5vw, .75rem)",
				"medium": "clamp(1rem, 8vw, calc(2 * var(--wp--style--block-gap)))",
				"large": "clamp(2rem, 10vw, 4rem)",
				"outer": "var(--wp--custom--spacing--small, .5rem)"
			},
			"typography": {
				"font-size": {
					"gigantic": "clamp(1.5rem, 6vw, 1.75rem)",
					"colossal": "clamp(1.65rem, 6vw, 2.5rem)",
					"normal": "1rem"
				},
				"line-height": {
					"tiny": 1,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.5,
                    "large": 1.7
				}
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": true,
            "customFontSize": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/montserrat-regular.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/montserrat-semiBold.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/montserrat-semiBoldItalic.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/montserrat-bold.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/montserrat-boldItalic.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/montserrat-black.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/montserrat-blackItalic.woff2"
							]
						}
					],
					"fontFamily": "Montserrat",
					"name": "Montserrat",
					"slug": "montserrat"
				},
                {
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/openSans-regular.woff2"
							]
						}
					],
					"fontFamily": "Open Sans",
					"name": "Open Sans",
					"slug": "open-sans"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/raleway-regular.woff2"
							]
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/raleway-italic.woff2"
							]
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/raleway-semiBold.woff2"
							]
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/raleway-semiBoldItalic.woff2"
							]
						}
					],
					"fontFamily": "Raleway, sans-serif",
					"name": "Raleway",
					"slug": "raleway"
				}
			],
			"fontSizes": [
				{
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"size": "1rem",
					"slug": "normal"
				},
				{
					"size": "clamp(1rem, 3vw, 1.125rem)",
					"slug": "medium"
				},
				{
					"size": "clamp(1.25rem, 3vw, 1.675rem)",
					"slug": "large"
				},
				{
					"size": "clamp(2.25rem, 3vw, 2.75rem)",
					"slug": "x-large"
				},
				{
					"size": "clamp(1.875rem, 8vw, 3.25rem)",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0",
					"width": "2px"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat-semibold)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"lineHeight": "var(--wp--custom--typography--line-height--large)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600"
				}
			},
            "core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--raleway)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"

				},
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "underline"
                        }
                    }
                }
			},
			"core/cover": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				}
            }
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--text-primary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--custom--spacing--small)",
						"bottom": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserra—Ç)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--links)"
				}
			},
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			}
		},
		"spacing": {
			"blockGap": ".75rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--raleway)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"fontStyle": "normal"
		}
	}
}
